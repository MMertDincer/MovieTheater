<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>

<AnchorPane prefHeight="400.0" prefWidth="391.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AdminPageController">
   <children>
      <TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Users">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="userTableView" layoutX="14.0" layoutY="36.0" prefHeight="280.0" prefWidth="300.0" />
                        <Line endX="-100.0" endY="320.0" layoutX="431.0" layoutY="31.0" startX="-100.0" startY="-20.0" />
                        <Label layoutX="414.0" layoutY="14.0" text="Register User" />
                        <Label layoutX="128.0" layoutY="14.0" text="Delete User" />
                        <TextField fx:id="usernameField" layoutX="372.0" layoutY="41.0" promptText="Username" />
                        <PasswordField fx:id="passwordField" layoutX="372.0" layoutY="76.0" promptText="Password" />
                        <Button fx:id="registerButton" layoutX="421.0" layoutY="112.0" mnemonicParsing="false" onMouseClicked="#registerButtonClicked" text="Register" />
                        <Button fx:id="deleteButton" layoutX="135.0" layoutY="326.0" mnemonicParsing="false" onMouseClicked="#deleteButtonClicked" text="Delete" />
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab text="Movies">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
            <Tab text="Reservations">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
